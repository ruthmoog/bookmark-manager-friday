<ul>
  <% @bookmarks.each do |bookmark| %>
   <li>
   <a href="<%= bookmark.url %>">
    <%= bookmark.title %>
   </a>
   <form action="/bookmarks/<%= bookmark.id %>" method="post">
     <input type='hidden' name='_method' value='delete'/>
     <input type="submit" value="delete" />
    </form>
   </li>
   <% end %>
</ul>
